{
 "asset_type": "shader",
 "location_attribute_glsl_name": "loc",
  "parameters": [
    {
      "glsl_name": "tex",
      "type": "texture",
      "name": "Texture",
      "default": "texturedmodel/green_square.texture",
      "description": "The texture to mask with"
    },
    {
      "glsl_name": "oncolor",
      "type": "vec4",
      "name": "On Color",
      "default": [1.0, 0.0, 0.0, 1.0],
      "description": "The color to show when the texture is being shown"
    },
    {
      "glsl_name": "offcolor",
      "type": "vec4",
      "name": "Off Color",
      "default": [0.0, 0.0, 0.0, 0.0],
      "description": "The color to show when the texture is not being shown"
    }
  ],
  "vertex_attributes": [
    {
      "type": "vec2",
      "glsl_name": "uvs",
      "name": "uv"
    }
  ]
}
